{% extends 'layout/autenticado.html.twig' %}
{% block stylesheets %}
    <link rel="stylesheet" href="{{ asset('plugins/openlayer/css/ol.css','app') }}" type="text/css">
    <style>
        .map {
            height: 400px;
            width: 100%;
        }
    </style>

{% endblock %}
{% block body %}
    <div id="map" class="map"></div>
{% endblock %}
{% block javascripts %}
    <script src="{{ asset('plugins/openlayer/js/ol.js','app') }}"></script>
    <script type="text/javascript">
        var map = new ol.Map({
            target: 'map',
            layers: [
                new ol.layer.Tile({
                    source: new ol.source.OSM()
                })
            ],
            view: new ol.View({
                center: ol.proj.fromLonLat([37.41, 8.82]),
                zoom: 4
            })
        });
        map.addControl(new ol.control.ZoomSlider());




    </script>
{% endblock %}