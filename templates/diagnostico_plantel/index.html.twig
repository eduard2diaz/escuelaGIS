                <td>{{ diagnostico_plantel.idcondiciontelefono }}</td>
                <td>{{ diagnostico_plantel.internet ? 'Yes' : 'No' }}</td>
                <td>{{ diagnostico_plantel.idcondicioninternet }}</td>
                <td>{{ diagnostico_plantel.iddiagnosticoplantel }}</td>
                <td>{{ diagnostico_plantel.diagnosticoarchivo }}</td>
                <td>
                    <a href="{{ path('diagnostico_plantel_show', {'id': diagnostico_plantel.id}) }}">show</a>
                    <a href="{{ path('diagnostico_plantel_edit', {'id': diagnostico_plantel.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="30">no records found</td>
            </tr>
        {% endfor %}
