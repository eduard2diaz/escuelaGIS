                <th>Diagnosticoarchivo</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for diagnostico_plantel in diagnostico_plantels %}
            <tr>
                <td>{{ diagnostico_plantel.id }}</td>
                <td>{{ diagnostico_plantel.numeroaulas }}</td>
                <td>{{ diagnostico_plantel.idcondicionesAula }}</td>
                <td>{{ diagnostico_plantel.numerosanitarios }}</td>
                <td>{{ diagnostico_plantel.idcondicionessanitarios }}</td>
                <td>{{ diagnostico_plantel.numerooficinas }}</td>
