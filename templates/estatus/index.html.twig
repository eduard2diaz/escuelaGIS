<!DOCTYPE html>

<title>Estatus index</title>

{% block body %}
    <h1>Estatus index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Estatus</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for estatus in estatuses %}
            <tr>
                <td>{{ estatus.id }}</td>
                <td>{{ estatus.estatus }}</td>
                <td>
                    <a href="{{ path('estatus_show', {'id': estatus.id}) }}">show</a>
                    <a href="{{ path('estatus_edit', {'id': estatus.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('estatus_new') }}">Create new</a>
{% endblock %}
